server {
        listen 12000 default_server;
        listen [::]:12000 default_server;

        server_name _;
        include /etc/nginx/mime.types;

        location /1.0/identifiers {
                proxy_pass http://127.0.0.1:12002;
        }
        location / {
                proxy_pass http://did.dyne.org:12001;
        }
}
